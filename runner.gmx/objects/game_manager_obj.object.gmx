    draw_set_color(c_black);
    draw_set_alpha(0.8);
    draw_rectangle(view_wview[0] - string_width(debugString), view_hview[0] - string_height(debugString), view_wview[0], view_hview[0], false);
    
    draw_set_color(c_white);
    draw_set_alpha(1.0);
    draw_set_halign(fa_right);
    draw_set_valign(fa_bottom);
    draw_text(view_wview[0], view_hview[0], debugString);
}
